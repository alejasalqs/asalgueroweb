---
import Layout from '../layouts/Layout.astro';
import '../styles/global.css';
const { frontmatter } = Astro.props;
---

<Layout title={frontmatter.title}>
  <div class="post-container">
    <h2>{frontmatter.title}</h2>
    <slot />
  </div>
</Layout>
<style>
  ul li {
    list-style: circle;
  }
</style>
